{
  "name": "Slinky",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Share browsing with your Slack team",
  "homepage_url": "https://slack.com/",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icons/slinky.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://slack.com/*",
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": ["js/background.js"]
  },
  "icons": {
    "16": "icons/slinky16.png",
    "48": "icons/slinky48.png",
    "128": "icons/slinky.png"
  }
}
