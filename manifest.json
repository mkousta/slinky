{
  "name": "Slinky",
  "version": "0.2.1",
  "manifest_version": 2,
  "description": "Share browsing with your Slack team",
  "homepage_url": "https://slack.com/",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icons/slnk48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://slack.com/*",
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": ["js/background.js"]
  },
 "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+S",
        "mac": "Command+Shift+S",
        "chromeos": "Ctrl+Shift+S",
        "linux": "Ctrl+Shift+S"
      }
    }
  },
  "icons": {
    "16": "icons/slnk16.png",
    "32": "icons/slnk32.png",
    "48": "icons/slnk48.png",
    "128": "icons/slnk128.png"
  }
}
